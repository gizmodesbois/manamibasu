{
  "name": "Apprendre le japonais / 学びます日本語",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Cette extension vous permettra d'apprendre les kana, kanji et certaines phrases en japonais",
  "homepage_url": "https:/simonhenrotte.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "fr",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "options_page": "src/options/index.html",
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "bookmarks",
    "https://*/*"
  ]
}